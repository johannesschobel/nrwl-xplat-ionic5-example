{
  "version": 1,
  "projects": {
    "todos": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/todos",
      "sourceRoot": "apps/todos/src",
      "prefix": "nrwl-xplat-ionic5-example",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/todos",
            "index": "apps/todos/src/index.html",
            "main": "apps/todos/src/main.ts",
            "polyfills": "apps/todos/src/polyfills.ts",
            "tsConfig": "apps/todos/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/todos/src/favicon.ico", "apps/todos/src/assets"],
            "styles": ["apps/todos/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/todos/src/environments/environment.ts",
                  "with": "apps/todos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "todos:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "todos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todos:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/todos/tsconfig.app.json",
              "apps/todos/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/todos/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/todos/jest.config.js",
            "tsConfig": "apps/todos/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/todos/src/test-setup.ts"
          }
        }
      }
    },
    "todos-e2e": {
      "root": "apps/todos-e2e",
      "sourceRoot": "apps/todos-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/todos-e2e/cypress.json",
            "tsConfig": "apps/todos-e2e/tsconfig.e2e.json",
            "devServerTarget": "todos:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "todos:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/todos-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/todos-e2e/**"]
          }
        }
      }
    },
    "ionic-mobile": {
      "root": "apps/ionic-mobile/",
      "sourceRoot": "apps/ionic-mobile/src",
      "projectType": "application",
      "prefix": "nrwl-xplat-ionic5-example",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "apps/ionic-mobile/www",
            "index": "apps/ionic-mobile/src/index.html",
            "main": "apps/ionic-mobile/src/main.ts",
            "polyfills": "apps/ionic-mobile/src/polyfills.ts",
            "tsConfig": "apps/ionic-mobile/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/ionic-mobile/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              {
                "input": "apps/ionic-mobile/src/theme/variables.scss"
              },
              {
                "input": "apps/ionic-mobile/src/global.scss"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ionic-mobile/src/environments/environment.ts",
                  "with": "apps/ionic-mobile/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ionic-mobile:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ionic-mobile:build:production"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ionic-mobile:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/ionic-mobile/src/test.ts",
            "polyfills": "apps/ionic-mobile/src/polyfills.ts",
            "tsConfig": "apps/ionic-mobile/tsconfig.spec.json",
            "karmaConfig": "apps/ionic-mobile/karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "apps/ionic-mobile/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "apps/ionic-mobile/src/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ionic-mobile/tsconfig.app.json",
              "apps/ionic-mobile/tsconfig.spec.json",
              "apps/ionic-mobile/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/ionic-mobile/e2e/protractor.conf.js",
            "devServerTarget": "ionic-mobile:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ionic-mobile:serve:production"
            },
            "ci": {
              "devServerTarget": "ionic-mobile:serve:ci"
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "ionic-mobile:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ionic-mobile:build:production"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "ionic-mobile:ionic-cordova-build",
            "devServerTarget": "ionic-mobile:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "ionic-mobile:ionic-cordova-build:production",
              "devServerTarget": "ionic-mobile:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nstudio/xplat"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "todos"
}
